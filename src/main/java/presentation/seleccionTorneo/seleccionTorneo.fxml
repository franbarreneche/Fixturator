<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1" fx:controller="presentation.seleccionTorneo.SeleccionTorneoPresenter">
   <children>
      <VBox alignment="TOP_CENTER" spacing="10.0">
         <children>
            <Label text="SELECCION DE TORNEO">
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
            </Label>
            <TreeView fx:id="arbolTorneos" prefHeight="200.0" prefWidth="200.0" />
            <Button mnemonicParsing="false" onAction="#verTorneo" text="Ver" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
